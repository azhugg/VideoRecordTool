                            <Option name="roll" text="滚动" font="0" bkcolor="FF3275EE#" borderround="5,5" group="camera"/>
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                            <Option name="reset_camera" text="恢复默认" font="0" bkcolor="FF3275EE#" group="camera" borderround="5,5" />
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="55" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
                            <VerticalLayout>
                                <HorizontalLayout>
                                    <VerticalLayout width="5" />
                                    <Button name="camera_dec" width="45" height="25" normalimage="file='image\slider\left.bmp'" />
                                    <VerticalLayout width="5" />
                                    <Slider name="camera_slider" min="0" max="5" step="1" height="20" valign="center"/>
                                    <VerticalLayout width="5" />
                                    <Button name="camera_inc" width="45" height="25" normalimage="file='image\slider\right.bmp'" />
                                    <VerticalLayout width="5" />
                                </HorizontalLayout>
                                <HorizontalLayout>
                                    <VerticalLayout width="50" />
                                    <Label name="camera_min" text="0" font="0" align="left" valign="center" textcolor="#FF000000"  />
                                    <Label name="camera_val" text="10" font="0" align="center" valign="center" textcolor="#FF000000"  />
                                    <Label name="camera_max" text="256" font="0" align="right" valign="center" textcolor="#FF000000"  />
                                    <VerticalLayout width="50" />
                                </HorizontalLayout>
                            </VerticalLayout>
                            <Option name="camera_auto" text="自动" enabled="false" width="100" font="0" bkcolor="FF3275EE#" borderround="5,5" />
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                    </VerticalLayout>
                    <VerticalLayout> <!--语言设置-->
                        <Label text="语言设置" font="0" align="center" valign="center" textcolor="#FF000000"  />
