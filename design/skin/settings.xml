<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window size="800,500" mininfo="800,500" maxinfo="800,500" caption="0,0,0,1" sizebox="0,0,0,0">
    <Font shared="true" id="0" name="微软雅黑" size="26" bold="false" />
	<Font shared="true" id="1" name="幼圆" size="30" />
    <Default shared="true" name="Slider" value="thumbsize=&quot;30,12&quot; bkimage=&quot;file='image\slider\bg.bmp' corner='6,0,6,0' mask='#FFFF00FF'&quot; foreimage=&quot;file='image\slider\bg.bmp' corner='6,0,6,0' mask='#FFFF00FF'&quot; thumbimage=&quot;file='image\slider\thumb.bmp' source='0,0,66,103' mask='#FFFFD966'&quot; thumbhotimage=&quot;file='image\slider\thumb.bmp' source='0,0,66,100' mask='#FFFFD966'&quot; " />
	<HorizontalLayout bkcolor="FFD77E44#" bordersize="1" bordercolor="#FF707070"> <!--整个窗口-->
        <VerticalLayout width="180" bkcolor="FFFFD966#" bordersize="1" bordercolor="#FF707070"> <!--左侧列表-->
            
            <HorizontalLayout height="25" />
            <HorizontalLayout>
                <VerticalLayout width="5" />
                <VerticalLayout>
                    <Option name="resolution" text="分辨率" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab" selected="true"/> <HorizontalLayout height="5" />
                    <Option name="record" text="录像选项" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                    <Option name="video" text="视频设置" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                    <Option name="camera" text="摄像机设置" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                    <Option name="language" text="语言设置" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                    <Option name="time" text="时间设置" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                    <Option name="recover" text="恢复设置" font="0" bkcolor="FF3275EE#" borderround="15,15" group="contenttab"/> <HorizontalLayout height="5" />
                </VerticalLayout>
                <VerticalLayout width="5" />
           </HorizontalLayout>
           <HorizontalLayout height="15" />
        </VerticalLayout>
        
        <VerticalLayout> <!--内容-->
            <HorizontalLayout height="15" />
            <HorizontalLayout name="container" >
                <VerticalLayout width="20" />
                <TabLayout name="options">
                    <VerticalLayout> <!--分辨率-->
                        <Label text="分辨率格式yuv/mpeg" font="0" align="center" valign="center" textcolor="#FF000000"  />
                        <HorizontalLayout height="15" />
                        <HorizontalLayout>
                            <VerticalLayout width="15" />
                            <VerticalLayout name="resolutions">
                            </VerticalLayout>
                            <VerticalLayout width="15" />
                            <VerticalLayout name="cap_mode">
                            </VerticalLayout>
                            <VerticalLayout width="15" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                    </VerticalLayout>
                    <VerticalLayout> <!--录像选项-->
                        <Label text="录像选项" font="0" align="center" valign="center" textcolor="#FF000000"  />
                        <HorizontalLayout height="25" />
                        <VerticalLayout bkcolor="FFFFD966#" bordersize="1" bordercolor="#FF707070">
                            <Label text="时长（值）" font="0" align="center" valign="center" textcolor="#FF000000"  />
                            <HorizontalLayout>
                                <VerticalLayout width="25" />
                                <Label text="max" font="0" align="center" valign="center" textcolor="#FF000000"  />
                                <VerticalLayout width="320"/>
                                <Label text="60min" font="0" align="center" valign="center" textcolor="#FF000000"  />
                                <VerticalLayout width="25" />
                            </HorizontalLayout>
                            <HorizontalLayout height="15" />
                            <HorizontalLayout>
                                <VerticalLayout width="5" />
                                <Button name="rec_time_dec" width="45" height="25" normalimage="file='image\slider\left.bmp'" />
                                <VerticalLayout width="5" />
                                <Slider name="rec_time" min="0" max="60" step="1" height="20" valign="center"/>
                                <VerticalLayout width="5" />
                                <Button name="rec_time_inc" width="45" height="25" normalimage="file='image\slider\right.bmp'" />
                                <VerticalLayout width="5" />
                            </HorizontalLayout>
                            <HorizontalLayout height="25" >
                                <Label name="rec_time_text" text="33" font="0" align="center" valign="center" textcolor="#FF000000"  />
                            </HorizontalLayout>
                            <HorizontalLayout />
                        </VerticalLayout>
                        <HorizontalLayout height="15" />
                    </VerticalLayout>
                    <VerticalLayout> <!--视频设置-->
                        <Label text="视频设置" font="0" align="center" valign="center" textcolor="#FF000000"  />
