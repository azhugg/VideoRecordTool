            <HorizontalLayout height="35">
                <VerticalLayout />
                <Button name="closebtn" text="关闭" font="0" width="100" bkcolor="FF3275EE#" borderround="5,5" />
                <VerticalLayout width="5"/>
            </HorizontalLayout>
            <HorizontalLayout height="5" />
        </VerticalLayout>
	</HorizontalLayout>
</Window>




















