                            </VerticalLayout>
                            <VerticalLayout width="180" />
                        </HorizontalLayout>
                    </VerticalLayout>
                    <VerticalLayout> <!--时间设置-->
                        <Label text="时间设置" font="0" align="center" valign="center" textcolor="#FF000000"  />
                        <HorizontalLayout height="80" />
                        <HorizontalLayout>
                            <VerticalLayout width="50" />
                            <Option name="year" selected="true" text="2017年" font="0" bkcolor="FF3275EE#" borderround="5,5" group="time" />
                            <VerticalLayout width="50" />
                            <Option name="month" text="05月" font="0" bkcolor="FF3275EE#" borderround="5,5" group="time" />
                            <VerticalLayout width="50" />
                            <Option name="day" text="26日" font="0" bkcolor="FF3275EE#" borderround="5,5" group="time" />
                            <VerticalLayout width="50" />
                        </HorizontalLayout>
                        <HorizontalLayout height="50" />
                        <HorizontalLayout>
                            <VerticalLayout width="50" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="50" />
                            <Option name="hour" text="13时" font="0" bkcolor="FF3275EE#" borderround="5,5" group="time" />
                            <VerticalLayout width="50" />
                            <Option name="minute" text="37分" font="0" bkcolor="FF3275EE#" borderround="5,5" group="time" />
