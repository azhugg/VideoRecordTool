                            <Option name="exposure" text="曝光" font="0" bkcolor="FF3275EE#" borderround="5,5" group="camera"/>
                            <VerticalLayout width="5" />
                            <Option name="iris" text="光圈" font="0" bkcolor="FF3275EE#" borderround="5,5" group="camera"/>
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
