                        <HorizontalLayout height="50" />
                        <HorizontalLayout>
                            <VerticalLayout width="80" />
                            <Button name="reset_ok" text="确定" font="0" bkcolor="FF3275EE#" borderround="5,5" />
                            <VerticalLayout width="50" />
                            <Button name="reset_cancel" text="取消" font="0" bkcolor="FF3275EE#" borderround="5,5" />
