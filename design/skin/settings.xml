                            <Option name="contrast" text="对比度" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Option name="hue" text="色调" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Option name="saturation" text="饱和度" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
                            <Option name="sharpness" text="清晰度" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Option name="gamma" text="伽马" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Option name="white_balance" text="白平衡" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Option name="backlight" text="逆光对比" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
                            <Option name="gain" text="增益" font="0" bkcolor="FF3275EE#" borderround="5,5" group="video"/>
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                            <Button name="" enabled="false" text="" font="0"  borderround="5,5" />
                            <VerticalLayout width="5" />
                            <Option name="reset_video" text="恢复默认" font="0" bkcolor="FF3275EE#" group="video" borderround="5,5" />
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="55" />
                        <HorizontalLayout>
                            <VerticalLayout width="5" />
                            <VerticalLayout>
                                <HorizontalLayout>
                                    <VerticalLayout width="5" />
                                    <Button name="video_dec" width="45" height="25" normalimage="file='image\slider\left.bmp'" />
                                    <VerticalLayout width="5" />
                                    <Slider name="video_slider" min="0" max="5" step="1" height="20" valign="center"/>
                                    <VerticalLayout width="5" />
                                    <Button name="video_inc" width="45" height="25" normalimage="file='image\slider\right.bmp'" />
                                    <VerticalLayout width="5" />
                                </HorizontalLayout>
                                <HorizontalLayout>
                                    <VerticalLayout width="50" />
                                    <Label name="video_min" text="0" font="0" align="left" valign="center" textcolor="#FF000000"  />
                                    <VerticalLayout width="50" />
                                    <Label name="video_val" text="10" font="0" align="center" valign="center" textcolor="#FF000000"  />
                                    <VerticalLayout width="50" />
                                    <Label name="video_max" text="256" font="0" align="right" valign="center" textcolor="#FF000000"  />
                                    <VerticalLayout width="50" />
                                </HorizontalLayout>
                            </VerticalLayout>
                            <Option name="video_auto" text="自动" enabled="false" width="100" font="0" bkcolor="FF3275EE#" borderround="5,5" />
                            <VerticalLayout width="5" />
                        </HorizontalLayout>
                        <HorizontalLayout height="15" />
                    </VerticalLayout>
                    <VerticalLayout> <!--摄像机设置-->
                        <Label text="摄像机设置" font="0" align="center" valign="center" textcolor="#FF000000"  />
